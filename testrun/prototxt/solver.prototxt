net: "prototxt/pspnet101_cityscapes_713_caffe.prototxt"
# The base learning rate, momentum and the weight decay of the network.

solver_type: SGD
base_lr: 0.01
momentum: 0.9
weight_decay: 0.0001
# The learning rate policy
lr_policy: "poly"
power: 0.9
iter_size: 1
display:1
# The maximum number of iterations
max_iter: 90000
# snapshot intermediate results
snapshot: 500
snapshot_prefix: "model/snapshot/train"
# solver mode: CPU or GPU
solver_mode: GPU
#device_id: [1, 2, 3]
#random_seed: 678654
